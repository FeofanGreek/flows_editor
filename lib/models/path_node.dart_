/// Класс для представления ячейки в алгоритме A*
class PathNode implements Comparable<PathNode> {
  PathNode({required this.cellIndex, required this.g, required this.h, this.parent});
  final int cellIndex;
  final double g; // стоимость от начала
  final double h; // эвристика до конца
  final PathNode? parent;

  double get f => g + h;

  @override
  int compareTo(PathNode other) => f.compareTo(other.f);
}
